<%= button_to "Log out", destroy_user_session_path, :method => :delete %>
<h4><%= link_to "Income Statement", income_statements_path %> |
<%= link_to "Balance Sheet", balance_sheets_path %></h4>
<h1>Your Dashboard</h1>

<%= form_for @entrybuilder, url: entries_path, as: :entry do |f| %>
  Amount: <%= f.text_field :amount %><br>
  Date: <%= f.date_field :date %><br>
  Debit: <%= select_tag "entry[debit_account]", options_from_collection_for_select(@user.accounts.all, "name", "name") %><br>
  Credit: <%= select_tag "entry[credit_account]", options_from_collection_for_select(@user.accounts.all, "name", "name") %><br>
  <%= f.submit "Create Journal Entry" %>
<% end %>
